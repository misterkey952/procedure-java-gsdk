<?xml version="1.0" encoding="UTF-8"?>
<access namespace="century.gsdk.storage.test" storage="s_account" table="account" name="Account">
    <struct name="SubAccount">
        <field name="id" type="string"/>
        <field name="opertor_id" type="string"/>
        <field name="login_name" type="string"/>
    </struct>

    <method name="selectAccount" return="AccountStruct" mapKey="id">
        <param name="sa" type="SubAccount"/>
        <sql>select * from #table where id=?sa.id? and opera=?sa.opertor_id?</sql>
    </method>
</access>
